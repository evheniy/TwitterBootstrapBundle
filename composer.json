{
    "name": "evheniy/twitter-bootstrap-bundle",
    "description": "The TwitterBootstrapBundle adds the ability to add latest Twitter Bootstrap to your application.",
    "keywords": ["Twitter Bootstrap", "Twitter","Bootstrap", "Symfony2", "latest"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name":     "evheniy",
            "homepage": "https://github.com/evheniy"
        }
    ],
    "require": {
        "php":             ">=5.3.0",
        "symfony/symfony": ">=2.3",
        "evheniy/jquery-bundle": ">=1.0"
    },
    "require-dev": {
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Evheniy\\TwitterBootstrapBundle": ""
        }
    },
    "target-dir" : "Evheniy/TwitterBootstrapBundle"
}
